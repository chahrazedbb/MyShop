<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tp.myshope">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".addBooks"></activity>
        <activity android:name=".addImages"></activity>
        <activity android:name=".addMusic"></activity>
        <activity android:name=".AddBooksToCart"></activity>
        <activity android:name=".AddImagesToCart"></activity>
        <activity android:name=".AddMusicToCart"></activity>
        <activity android:name=".ShoppingList"></activity>
        <activity android:name=".EditBook"></activity>
        <activity android:name=".EditImage"></activity>
        <activity android:name=".EditMusic"></activity>
    </application>

</manifest>